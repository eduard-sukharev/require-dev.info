---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - post
    - posts
    - tagList
  addAllPagesToCollections: true
eleventyComputed:
  title: Tagged “{{ tag }}”
permalink: /tags/{{ tag | slugify }}/
---

{% extends 'layouts/base.njk' %}

{% block body %}
  <h1>{{ title }}</h1>

{% set packages = collections[ tag ] %}
{% include "packages_list.njk" %}

<p>See <a href="{{ '/tags/' | url }}">all tags</a>.</p>

{% endblock %}
