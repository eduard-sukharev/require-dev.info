{
  "name": "eleventy-base-blog",
  "version": "7.0.0",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "serve": "npx @11ty/eleventy --serve --quiet & npx tailwindcss -i ./tailwind/tailwind.css -o ./_site/css/tailwind.css --watch",
    "start": "npx @11ty/eleventy --serve --quiet & npx tailwindcss -i ./tailwind/tailwind.css -o ./_site/css/tailwind.css --watch",
    "build": "rm -rf _site && ELEVENTY_ENV=production npx @11ty/eleventy && npx tailwindcss -i ./tailwind/tailwind.css -o ./_site/css/tailwind.css --minify"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "flat-cache": "^5.0.0",
    "luxon": "^2.4.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.4"
  }
}
